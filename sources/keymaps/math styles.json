// add style to text in mathmode

{ "keys": ["1", "d"], "command": "addenv", "args": {"env_name": "dot"}, "context":
    [
        { "key": "selector", "operator": "equal", "operand": "text.tex.latex meta.environment.math" }
    ]
},

{ "keys": ["2", "d"], "command": "addenv", "args": {"env_name": "ddot"}, "context":
    [
        { "key": "selector", "operator": "equal", "operand": "text.tex.latex meta.environment.math" }
    ]
},